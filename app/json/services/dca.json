    "command": "dca", 
    "inputs": {
        "parameters": {
            "seq": {
                "ismandatory": true, 
                "prompt": "Sequences File", 
                "type": {
                    "datatype_class": "Sequence", 
                    "data_formats": [
                        "FASTA"
                    ]
                }, 
                "format": {
                    "perl": "\" $value\"", 
                    "python": "\" \"+str(value)"
                }
            }, 
            "control": {
                "name": "control", 
                "parameters": {
                    "cost": {
                        "prompt": "Cost matrix (-c)", 
                        "type": {
                            "datatype_class": "Choice"
                        }, 
                        "default_value": [
                            "null"
                        ], 
                        "authorized_values": [
                            {
                                "value": "null", 
                                "label": "Default: depends on sequences"
                            }, 
                            {
                                "value": "blosum30", 
                                "label": "BLOSUM30"
                            }, 
                            {
                                "value": "blosum45", 
                                "label": "BLOSUM45"
                            }, 
                            {
                                "value": "blosum62", 
                                "label": "BLOSUM62"
                            }, 
                            {
                                "value": "pam160", 
                                "label": "PAM160"
                            }, 
                            {
                                "value": "pam250", 
                                "label": "PAM250"
                            }, 
                            {
                                "value": "unitcost", 
                                "label": "Unitcost"
                            }, 
                            {
                                "value": "dna", 
                                "label": "DNA"
                            }, 
                            {
                                "value": "rna", 
                                "label": "RNA"
                            }, 
                            {
                                "value": "dnarna", 
                                "label": "DNA/RNA"
                            }
                        ], 
                        "format": {
                            "perl": "(defined $value and $value ne $vdef)? \" -c $value\" : \"\" ", 
                            "python": "( \"\"  , \" -c \" + str(value) )[ value is not None and value != vdef]"
                        }
                    }, 
                    "gaps": {
                        "prompt": "Penalize end gaps as internal gaps (-g)", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "perl": " ($value)? \" -g \":\"\"", 
                            "python": "(\"\" , \" -g \")[ value ]"
                        }, 
                        "comment": "<text lang=\"en\">Default: free shift</text> </comment>"
                    }, 
                    "approximate": {
                        "prompt": "Use approximate cut positions (-a)", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "perl": " ($value)? \" -a\":\"\"", 
                            "python": "(\"\" , \" -a\")[ value ]"
                        }, 
                        "comment": "<text lang=\"en\">On: FastDCA (use approximate cut positions); Off: slower, more accurate algorithm (search for exact cut positions)</text> </comment>"
                    }, 
                    "intensity": {
                        "prompt": "Weight intensity (-b)", 
                        "type": {
                            "datatype_class": "Float"
                        }, 
                        "default_value": [
                            "0.0"
                        ], 
                        "format": {
                            "perl": "(defined $value and $value != $vdef)? \" -b $value\" : \"\" ", 
                            "python": "( \"\"  , \" -b \" + str(value) )[ value is not None and value != vdef]"
                        }, 
                        "ctrl": {
                            "perl": "= 1.0", 
                            "python": "= 1.0"
                        }
                    }, 
                    "recursion": {
                        "prompt": "Recursion stop size (-l)", 
                        "type": {
                            "datatype_class": "Integer"
                        }, 
                        "default_value": [
                            "30"
                        ], 
                        "format": {
                            "perl": " (defined $value and $value != $vdef)? \" -l $value\" : \"\" ", 
                            "python": "( \"\"  , \" -l \" + str(value) )[ value is not None and value != vdef]"
                        }, 
                        "comment": "<text lang=\"en\">5 ... 100 recommended; small: faster algorithm, maybe worse.</text> </comment>"
                    }, 
                    "window": {
                        "prompt": "Window size (-w)", 
                        "type": {
                            "datatype_class": "Integer"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "perl": "(defined $value and  $value != $vdef)? \" -w $value\" : \"\" ", 
                            "python": "( \"\"  , \" -w \" + str(value) )[ value is not None and value != vdef]"
                        }, 
                        "comment": "<text lang=\"en\">To correct the alignment in the proximity of division sites, the sequences can be re-aligned inside a window of size w >= 0 placed across each slicing site.</text> </comment>"
                    }
                }
            }, 
            "output": {
                "name": "output", 
                "parameters": {
                    "quiet": {
                        "ishidden": true, 
                        "type": {
                            "datatype_class": "String"
                        }, 
                        "format": {
                            "perl": "\" -q\"", 
                            "python": "\" -q\""
                        }
                    }, 
                    "output_format": {
                        "prompt": "Output format (-f)", 
                        "type": {
                            "datatype_class": "Choice"
                        }, 
                        "default_value": [
                            "2"
                        ], 
                        "authorized_values": [
                            {
                                "value": "1", 
                                "label": "aln (1)"
                            }, 
                            {
                                "value": "2", 
                                "label": "fasta (2)"
                            }, 
                            {
                                "value": "3", 
                                "label": "nex (3)"
                            }, 
                            {
                                "value": "4", 
                                "label": "dca (4)"
                            }
                        ], 
                        "format": {
                            "perl": "(defined $value and $value ne $vdef)? \" -f $value\" : \"\" ", 
                            "python": "( \"\"  , \" -f \" + str(value) )[ value is not None and value != vdef]"
                        }
                    }, 
                    "suppress_output": {
                        "ishidden": true, 
                        "prompt": "Suppress output about progress of the program (-o)", 
                        "type": {
                            "datatype_class": "String"
                        }, 
                        "format": {
                            "perl": "\" -o\"", 
                            "python": "\" -o\""
                        }
                    }
                }
            }
        }
    }, 
    "outputs": {
        "parameters": {
            "fasta_outfile": {
                "isstdout": true, 
                "prompt": "Alignment file", 
                "type": {
                    "datatype_class": "Alignment", 
                    "data_formats": [
                        "\n          ", 
                        "\n          ", 
                        "\n        "
                    ]
                }, 
                "precond": {
                    "perl": "$output_format eq \"2\" or $output_format eq \"3\"", 
                    "python": "output_format == \"2\" or output_format == \"3\""
                }, 
                "filenames": {
                    "perl": "\"dca.out\"", 
                    "python": "\"dca.out\""
                }
            }, 
            "aln_outfile": {
                "isstdout": true, 
                "prompt": "DCA alignment file", 
                "type": {
                    "datatype_class": "Dcalignment", 
                    "datatype_superclass": "AbstractText", 
                    "data_formats": [
                        "\n          ", 
                        "\n          ", 
                        "\n        "
                    ]
                }, 
                "precond": {
                    "perl": "$output_format eq \"1\" or $output_format eq \"4\"", 
                    "python": "output_format == \"1\" or output_format == \"4\""
                }, 
                "filenames": {
                    "perl": "\"dca.out\"", 
                    "python": "\"dca.out\""
                }
            }
        }
    }, 
    "comment": "<text lang=\"en\">Divide-and-Conquer Multiple Sequence Alignment (DCA) is a program for producing fast, high quality simultaneous multiple sequence alignments of amino acid, RNA, or DNA sequences. The program is based on the DCA algorithm, a heuristic approach to sum-of-pairs (SP) optimal alignment that has been developed at the FSPM over the years 1995-97.</text> </comment>"
}