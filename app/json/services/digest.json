    "command": "digest", 
    "inputs": {
        "parameters": {
            "e_input": {
                "name": "e_input", 
                "parameters": {
                    "e_seqall": {
                        "ismandatory": true, 
                        "prompt": "seqall option", 
                        "type": {
                            "biotype": "Protein", 
                            "datatype_class": "Sequence", 
                            "data_formats": [
                                "EMBL", 
                                "FASTA", 
                                "GCG", 
                                "GENBANK", 
                                "NBRF", 
                                "PIR", 
                                "RAW", 
                                "SWISSPROT", 
                                "GFF"
                            ], 
                            "card": "1,n"
                        }, 
                        "format": {
                            "python": "(\"\", \" -seqall=\" + str(value))[value is not None]"
                        }
                    }, 
                    "e_mwdata": {
                        "prompt": "Molecular weights data file", 
                        "type": {
                            "datatype_class": "MolecularWeights", 
                            "datatype_superclass": "AbstractText"
                        }, 
                        "format": {
                            "python": "(\"\", \" -mwdata=\" + str(value))[value is not None ]"
                        }, 
                        "comment": "<text lang=\"en\">Molecular weight data for amino acids</text> </comment>"
                    }
                }
            }, 
            "e_required": {
                "name": "e_required", 
                "parameters": {
                    "e_menu": {
                        "ismandatory": true, 
                        "prompt": "Enzymes and reagents", 
                        "type": {
                            "datatype_class": "Choice"
                        }, 
                        "default_value": [
                            "1"
                        ], 
                        "authorized_values": [
                            {
                                "value": "1", 
                                "label": "Trypsin"
                            }, 
                            {
                                "value": "2", 
                                "label": "Lys-c"
                            }, 
                            {
                                "value": "3", 
                                "label": "Arg-c"
                            }, 
                            {
                                "value": "4", 
                                "label": "Asp-n"
                            }, 
                            {
                                "value": "5", 
                                "label": "V8-bicarb"
                            }, 
                            {
                                "value": "6", 
                                "label": "V8-phosph"
                            }, 
                            {
                                "value": "7", 
                                "label": "Chymotrypsin"
                            }, 
                            {
                                "value": "8", 
                                "label": "Cnbr"
                            }
                        ], 
                        "format": {
                            "python": "(\"\", \" -menu=\" + str(value))[value is not None and value!=vdef]"
                        }
                    }, 
                    "e_mono": {
                        "prompt": "Use monoisotopic weights", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "python": "(\"\", \" -mono\")[ bool(value) ]"
                        }
                    }
                }
            }, 
            "e_advanced": {
                "name": "e_advanced", 
                "parameters": {
                    "e_unfavoured": {
                        "prompt": "Allow unfavoured cuts", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "python": "(\"\", \" -unfavoured\")[ bool(value) ]"
                        }, 
                        "comment": "<text lang=\"en\">Trypsin will not normally cut after 'KR' if they are followed by any of 'KRIFLP'. Lys-C will not normally cut after 'K' if it is followed by 'P'. Arg-C will not normally cut after 'R' if it is followed by 'P'. V8-bicarb will not normally cut after 'E' if it is followed by any of 'KREP'. V8-phosph will not normally cut after 'DE' if they are followed by 'P'. Chymotrypsin will not normally cut after 'FYWLM' if they are followed by 'P'. Specifying unfavoured shows these unfavoured cuts as well as the favoured ones.</text> </comment>"
                    }, 
                    "e_ragging": {
                        "prompt": "Allow ragging", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "python": "(\"\", \" -ragging\")[ bool(value) ]"
                        }, 
                        "comment": "<text lang=\"en\">Allows semi-specific and non-specific digestion. This option is particularly useful for generating lists of peptide sequences for protein identification using mass-spectrometry.</text> </comment>"
                    }, 
                    "e_termini": {
                        "prompt": "Ragging value (value from 1 to 4)", 
                        "type": {
                            "datatype_class": "Choice"
                        }, 
                        "default_value": [
                            "1"
                        ], 
                        "authorized_values": [
                            {
                                "value": "1", 
                                "label": "None"
                            }, 
                            {
                                "value": "2", 
                                "label": "Nterm"
                            }, 
                            {
                                "value": "3", 
                                "label": "Cterm"
                            }, 
                            {
                                "value": "4", 
                                "label": "Nterm or cterm"
                            }
                        ], 
                        "format": {
                            "python": "(\"\", \" -termini=\" + str(value))[value is not None and value!=vdef]"
                        }
                    }
                }
            }, 
            "e_output": {
                "name": "e_output", 
                "parameters": {
                    "e_overlap": {
                        "prompt": "Show overlapping partials", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "python": "(\"\", \" -overlap\")[ bool(value) ]"
                        }, 
