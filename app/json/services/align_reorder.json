{
    "type": "program", 
    "name": "align_reorder", 
    "version": "6.3.1", 
    "title": "alignment entries reordering", 
    "description": "<text lang=\"en\">Reorders the entries of an MSA</text> </description>", 
    "authors": "N\u00e9ron, B.</authors>", 
    "references": [
        {
            "value": "EMBOSS: The European Molecular Biology Open Software Suite (2000) Rice,P. Longden,I. and Bleasby, A. Trends in Genetics 16, (6) pp276--277</reference>"
        }
    ], 
    "sourcelinks": [
        "http://emboss.sourceforge.net/download"
    ], 
    "homepagelinks": [
        "http://emboss.sourceforge.net"
    ], 
    "doclinks": [
        "http://bioweb2.pasteur.fr/docs/EMBOSS/abiview.html", 
        "http://emboss.sourceforge.net/docs/themes"
    ], 
    "mobyle_cats": [
        "display", 
        "alignment:formatter"
    ], 
    "command": "align_reorder", 
    "inputs": {
        "parameters": {
            "fasta_align": {
                "ismandatory": true, 
                "prompt": "alignment", 
                "type": {
                    "datatype_class": "Alignment", 
                    "data_formats": [
                        "FASTA"
                    ]
                }, 
                "format": {
                    "perl": "\" -a $value\"", 
                    "python": "\" -a \" + str( value )"
                }
            }, 
            "fasta_sequences": {
                "ismandatory": true, 
                "prompt": "sequences", 
                "type": {
                    "datatype_class": "Sequence", 
                    "data_formats": [
                        "FASTA"
                    ]
                }, 
                "format": {
                    "perl": "\" -s $value\"", 
                    "python": "\" -s \" + str( value )"
                }
            }
        }
    }, 
    "outputs": {
        "parameters": {
            "reordered_alignment": {
                "isstdout": true, 
                "prompt": "reordered alignment", 
                "type": {
                    "datatype_class": "Alignment", 
                    "data_formats": [
                        "FASTA"
                    ]
                }, 
                "filenames": {
                    "perl": "\"align_reorder.out\"", 
                    "python": "\"align_reorder.out\""
                }
            }
        }
    }, 
    "comment": "<{http://www.w3.org/1999/xhtml}div >This tool reorders the entries of an MSA according to a reference set of sequences.</{http://www.w3.org/1999/xhtml}div> </comment>"
}