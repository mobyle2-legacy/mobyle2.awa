    "inputs": {
        "parameters": {
            "e_input": {
                "name": "e_input", 
                "parameters": {
                    "e_sequence": {
                        "ismandatory": true, 
                        "prompt": "sequence option", 
                        "type": {
                            "biotype": "DNA", 
                            "datatype_class": "Sequence", 
                            "data_formats": [
                                "EMBL", 
                                "FASTA", 
                                "GCG", 
                                "GENBANK", 
                                "NBRF", 
                                "PIR", 
                                "RAW", 
                                "SWISSPROT", 
                                "GFF"
