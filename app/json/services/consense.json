    "inputs": {
        "parameters": {
            "consense": {
                "ishidden": true, 
                "type": {
                    "datatype_class": "String"
                }, 
                "format": {
                    "perl": "consense.params\"", 
                    "python": "consense.params\""
                }
            }, 
            "infile": {
                "ismaininput": true, 
                "ismandatory": true, 
                "prompt": "Series of trees in file (intree)", 
                "type": {
                    "datatype_class": "Tree", 
                    "data_formats": [
                        "NEWICK"
                    ]
                }, 
                "format": {
                    "perl": " \"", 
                    "python": "\""
                }, 
                "comment": "<text lang=\"en\">Input is a tree file which contains a series of trees in the Newick standard form</text> </comment>"
            }, 
            "type": {
                "prompt": "Consensus type", 
                "type": {
                    "datatype_class": "Choice"
                }, 
                "default_value": [
                    "MRE"
                ], 
                "authorized_values": [
                    {
                        "value": "MRE", 
                        "label": "Majority rule (extended)"
                    }, 
                    {
                        "value": "S", 
                        "label": "Strict"
                    }, 
                    {
                        "value": "MR", 
                        "label": "Majority rule"
                    }, 
                    {
                        "value": "ML", 
                        "label": "Ml (M-sub-L)"
                    }
                ]
            }, 
            "output": {
                "name": "output", 
                "parameters": {
                    "print_tree": {
                        "prompt": "Print out tree (3)", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "1"
                        ], 
                        "format": {
                            "perl": "($value) ? \"\" : \"3\\\\n\"", 
                            "python": "( \"3\\n\" , \"\" )[ value ]"
                        }, 
                        "comment": "<text lang=\"en\">Tells the program to print a semi-graphical picture of the tree in the outfile.</text> </comment>"
                    }, 
                    "print_treefile": {
                        "prompt": "Write out trees onto tree file (4)", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "1"
                        ], 
                        "format": {
                            "perl": "($value) ? \"\" : \"4\\\\n\"", 
                            "python": "( \"4\\n\" , \"\" )[ value ]"
                        }, 
                        "comment": "<text lang=\"en\">Tells the program to save the tree in a treefile (a standard representation of trees where the tree is specified by a nested pairs of parentheses, enclosing names and separated by commas).</text> </comment>"
                    }, 
                    "printdata": {
                        "prompt": "Print out the data at start of run (1)", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "perl": "($value) ? \"1\\\\n\" : \"\"", 
                            "python": "( \"\" , \"1\\n\" )[ value ]"
                        }
                    }
                }
            }, 
            "other_options": {
                "name": "other_options", 
                "parameters": {
                    "outgroup": {
                        "prompt": "Outgroup species (O)", 
                        "type": {
                            "datatype_class": "Integer"
                        }, 
                        "default_value": [
                            "1"
                        ], 
                        "format": {
                            "perl": "(defined $value and $value != $vdef) ? \"O\\\\n$value\\\\n\" : \"\"", 
                            "python": "( \"\" , \"O\\n\"+ str( value ) +\"\\n\" )[ value is not None and value != vdef ]"
                        }, 
                        "ctrl": {
                            "perl": " 0", 
                            "python": " 0"
                        }
                    }, 
                    "rooted": {
                        "prompt": "Trees to be treated as rooted (R)", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "perl": "($value) ? \"R\\\\n\" : \"\"", 
                            "python": "( \"\" , \"R\\n\" )[ value ]"
                        }
                    }
                }
            }, 
            "confirm": {
                "ishidden": true, 
                "type": {
                    "datatype_class": "String"
                }, 
                "format": {
                    "perl": "\"Y\\\\n\"", 
                    "python": "\"Y\\n\""
                }
            }, 
            "terminal_type": {
                "ishidden": true, 
                "type": {
                    "datatype_class": "String"
                }, 
                "format": {
                    "perl": "\"T\\\\n\"", 
                    "python": "\"T\\n\""
                }
            }
        }
    }, 
    "outputs": {
        "parameters": {
            "outfile": {
                "isout": true, 
                "prompt": "Consense output file", 
                "type": {
                    "datatype_class": "Text"
                }, 
                "format": {
                    "perl": " mv outfile consense.outfile\"", 
                    "python": " mv outfile consense.outfile \""
                }, 
                "filenames": {
                    "perl": "\"consense.outfile\"", 
                    "python": "\"consense.outfile\""
                }
            }, 
            "treefile": {
                "isout": true, 
                "prompt": "Consense tree file", 
                "type": {
                    "datatype_class": "Tree", 
                    "data_formats": [
                        "NEWICK"
                    ]
                }, 
                "precond": {
                    "perl": "$print_treefile", 
                    "python": "print_treefile"
                }, 
                "format": {
                    "perl": " mv outtree consense.outtree\"", 
                    "python": " mv outtree consense.outtree\""
                }, 
                "filenames": {
                    "perl": "\"consense.outtree\"", 
                    "python": "\"consense.outtree\""
                }
            }
        }
    }, 
    "comment": "<text lang=\"en\">CONSENSE reads a file of computer-readable trees and prints out (and may also write out onto a file) a consensus tree.</text> </comment>"
}