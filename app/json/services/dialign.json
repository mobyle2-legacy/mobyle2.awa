    "command": "dialign", 
    "inputs": {
        "parameters": {
            "sequence": {
                "ismandatory": true, 
                "prompt": "Sequences", 
                "type": {
                    "datatype_class": "Sequence", 
                    "data_formats": [
                        "FASTA"
                    ], 
                    "card": "2,n"
                }, 
                "format": {
                    "perl": "\" $value\"", 
                    "python": "\" \" + str(value)"
                }
            }, 
            "protein_dna": {
                "ismandatory": true, 
                "prompt": "Nucleic acid or protein alignment (-n)", 
                "type": {
                    "datatype_class": "Choice"
                }, 
                "default_value": [
                    "p"
                ], 
                "authorized_values": [
                    {
                        "value": "p", 
                        "label": "Protein (p)"
                    }, 
                    {
                        "value": "n", 
                        "label": "Nucleic (n)"
                    }
                ], 
                "format": {
                    "perl": "(defined $value and $value ne $vdef) ? \" -n\" : \"\"", 
                    "python": "( \"\"  , \" -n\" )[ value is not None and value != vdef]"
                }
            }, 
            "dialign_opt": {
                "name": "dialign_opt", 
                "parameters": {
                    "threshold": {
                        "prompt": "Threshold (-thr)", 
                        "type": {
                            "datatype_class": "Float"
                        }, 
                        "default_value": [
                            "0.0"
                        ], 
                        "format": {
                            "perl": "(defined $value and $value != $vdef) ? \" -thr $value\" : \"\"", 
                            "python": "( \"\"  , \" -thr \" + str(value) )[ value is not None and value != vdef]"
                        }
                    }, 
                    "cluster": {
                        "prompt": "Clustering type used to construct sequence tree", 
                        "type": {
                            "datatype_class": "Choice"
                        }, 
                        "default_value": [
                            "null"
                        ], 
                        "authorized_values": [
                            {
                                "value": "null", 
                                "label": "UPGMA"
                            }, 
                            {
                                "value": "max_link", 
                                "label": "Maximum linkage (-max_link)"
                            }, 
                            {
                                "value": "min_link", 
                                "label": "Minimum linkage (-min_link)"
                            }
                        ], 
                        "format": {
                            "perl": "(defined $value and $value ne $vdef) ? \" -$value\" : \"\"", 
                            "python": "( \"\"  , \" -\" +str(value) )[ value is not None and value !=vdef]"
                        }, 
                        "comment": "<text lang=\"en\">\"maximum or minimum linkage\" clustering used to construct sequence tree (instead of UPGMA).</text> </comment>"
                    }, 
                    "iterative": {
                        "prompt": "Iterative scoring scheme (-it)", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "perl": "($value) ? \" -it\" : \"\"", 
                            "python": "( \"\"  , \" -it\" )[ value ]"
                        }, 
                        "comment": "<text lang=\"en\">iterative scoring scheme (fragment scores are based on conditional probabilities given the previously aligned fragments. I.e. the probability of a fragment -- and therefore its score -- is not based on the probability of random occurrence in the input sequences but rather on the probability of occurrence between those fragments that have already been accepted in previous iteration steps).</text> </comment>"
                    }, 
                    "overlap": {
                        "prompt": "Overlap weights (-iw)", 
                        "type": {
                            "datatype_class": "Boolean"
                        }, 
                        "default_value": [
                            "0"
                        ], 
                        "format": {
                            "perl": "($value) ? \" -iw\" : \"\"", 
                            "python": "( \"\"  , \" -iw\" )[ value ]"
                        }, 
                        "comment": "<text lang=\"en\">overlap weights switched off (by default, overlap weights are used if up to 35 sequences are aligned). This option speeds up the alignment but may lead to reduced alignment quality.</text> </comment>"
                    }, 
                    "dna_opt": {
                        "name": "dna_opt", 
                        "precond": {
                            "perl": "$protein_dna eq \"n\"", 
                            "python": "protein_dna == \"n\""
                        }, 
                        "parameters": {
                            "translation": {
                                "prompt": "Translation of nucleotide diagonals into peptide diagonals (-nt)", 
                                "type": {
                                    "datatype_class": "Boolean"
                                }, 
                                "default_value": [
                                    "0"
                                ], 
                                "format": {
                                    "perl": "($value) ? \" -nt\" : \"\"", 
                                    "python": "( \"\"  , \" -nt\" )[ value ]"
                                }, 
                                "comment": "<text lang=\"en\">Input sequences are nucleic acid sequences and `nucleic acid segments' are translated to `peptide segments'.</text> </comment>"
                            }, 
                            "translation_strand": {
                                "prompt": "Strand to looked at Watson and Crick strands (-cs)", 
                                "type": {
                                    "datatype_class": "Boolean"
                                }, 
                                "precond": {
                                    "perl": "$translation or $mix", 
                                    "python": "translation or mix"
                                }, 
                                "default_value": [
                                    "0"
                                ], 
                                "format": {
                                    "perl": "($value) ? \" -cs\" : \"\"", 
                                    "python": "( \"\"  , \" -cs\" )[ value ]"
                                }, 
                                "comment": "<text lang=\"en\">If segments are translated, not only the `Watson strand' but also the `Crick strand' is looked at</text> </comment>"
                            }, 
                            "mix": {
                                "prompt": "Mixed alignments (-ma)", 
                                "type": {
                                    "datatype_class": "Boolean"
                                }, 
                                "default_value": [
                                    "0"
                                ], 
                                "format": {
                                    "perl": "($value) ? \" -ma\" : \"\"", 
                                    "python": "( \"\"  , \" -ma\" )[ value ]"
                                }, 
                                "comment": "<text lang=\"en\">`mixed alignments' consisting of P-fragments and N-fragments if nucleic acid sequences are aligned.</text> </comment>"
                            }, 
                            "speed": {
                                "prompt": "Dna alignment speed up (-ds)", 
                                "type": {
                                    "datatype_class": "Boolean"
                                }, 
                                "default_value": [
                                    "0"
                                ], 
                                "format": {
                                    "perl": "($value) ? \" -ds\" : \"\"", 
                                    "python": "( \"\"  , \" -ds\" )[ value ]"
                                }, 
                                "comment": "<text lang=\"en\">Non-translated nucleic acid fragments are taken into account only if they start with at least two matches. Speeds up DNA alignment at the expense of sensitivity.</text> </comment>"
                            }, 
                            "long_genomic": {
                                "prompt": "Long genomic sequences (-lgs)", 
                                "type": {
                                    "datatype_class": "Boolean"
                                }, 
